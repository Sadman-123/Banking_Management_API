{
    "version": 2,
    "builds": [
      {
        "src": "script.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/register",
        "methods": ["POST"],
        "dest": "/script.js"
      },
      {
        "src": "/insert",
        "methods": ["POST"],
        "dest": "/script.js"
      },
      {
        "src": "/api/(?<nm>[^/]+)",
        "methods": ["GET"],
        "dest": "/script.js"
      },
      {
        "src": "/api/sum/(?<nm>[^/]+)",
        "methods": ["GET"],
        "dest": "/script.js"
      }
    ]
  }
  