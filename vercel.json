{
    "version": 2,
    "builds": [
      {
        "src": "script.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/insert",
        "methods": ["POST"],
        "dest": "/api/insert"
      },
      {
        "src": "/api/(?<nm>[^/]+)",
        "methods": ["GET"],
        "dest": "/api/api"
      }
    ]
  }  