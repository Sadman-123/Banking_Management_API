{
    "version": 2,
    "builds": [
        {
            "src": "script.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/insert",
            "dest": "/script.js"
        },
        {
            "src": "/api/(?<nm>[^/]+)",
            "dest": "/script.js"
        }
    ]
}